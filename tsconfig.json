{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "public/*": [
        "public/*"
      ],
      "images/*": [
        "public/images/*"
      ],
      "icons/*": [
        "public/icons/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ]
    }
  }
}
